<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<classPathEntry location="D:/JDK/db/sqljdbc42.jar" />
	<context id="context1" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />

		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<!-- JDBC Connection -->
		<jdbcConnection
			driverClass="com.microsoft.sqlserver.jdbc.SQLServerDriver"
			connectionURL="jdbc:sqlserver://127.0.0.1:1433;DatabaseName=LINE;selectMethod=cursor"
			password="0503" userId="MiMi" />

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

	<!-- Model generator , generate the POJOs -->
	
		<javaModelGenerator targetPackage="com.line.demo.model"
			targetProject="LineProject" >
			<property name="trimStrings" value="true"/>
		</javaModelGenerator>
		<!-- sql map generator, generate the sql map XML files -->	
		<sqlMapGenerator targetPackage="com.line.demo.mapper" 
		targetProject="LineProject" />

	<!-- client generator , generate the DAO -->
		<javaClientGenerator targetPackage="com.line.demo.mapper"
			targetProject="LineProject" type="XMLMAPPER" />

	<!-- 沒有PK的 -->
		<!-- 
		<table  tableName="輸入資料表名稱"/>
		 -->
		
	<!-- 有PK的 -->
	
	<table tableName="USERS">
		<generatedKey column="PK" sqlStatement="SqlServer"
			identity="true"/>
	</table>
	</context>
</generatorConfiguration>